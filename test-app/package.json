    "ember-data": "5.3.3",
    "ember-element-helper": "^0.8.8",
    "ember-fetch": "^8.1.2",
    "ember-flatpickr": "workspace:*",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.4",
    "ember-power-select": "^7.2.0",
    "ember-qunit": "^8.1.1",
