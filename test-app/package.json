{
  "name": "test-app",
  "version": "5.0.0",
  "description": "An Ember addon that wraps the date picker Flatpickr",
  "keywords": [
    "flatpickr",
    "datepicker",
    "datetime picker"
  ],
  "homepage": "https://shipshapecode.github.io/ember-flatpickr",
  "repository": "https://github.com/shipshapecode/ember-flatpickr",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/preset-env": "^7.18.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "~3.2.0",
    "@embroider/macros": "^1.13.3",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.4.0",
    "ember-cli-addon-docs": "^6.0.2",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-code-coverage": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript": "^5.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-data": "4.12.4",
    "ember-fetch": "^8.0.2",
    "ember-flatpickr": "workspace:*",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-power-select": "^7.2.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.4.2",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "flatpickr": "^4.0.0",
    "loader.js": "^4.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "release-it": "^17.0.0",
    "release-it-lerna-changelog": "^5.0.0",
    "rsvp": "^4.8.5",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.0.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.15.1",
    "pnpm": "8.10.1"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.23.2"
  }
}
