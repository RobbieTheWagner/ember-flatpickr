  "greenkeeper": {
    "ignore": [
      "broccoli-asset-rev",
      "ember-cli-app-version",
      "ember-cli-babel",
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile",
      "ember-cli-inject-live-reload",
      "ember-cli-qunit",
      "ember-cli-sri",
      "ember-cli-test-loader",
      "ember-export-application-global",
      "ember-resolver",
      "loader.js"
    ]
  },
  "homepage": "https://shipshapecode.github.io/ember-flatpickr"
