{
  "name": "ember-flatpickr",
  "version": "3.2.2",
  "description": "An Ember addon that wraps the date picker Flatpickr",
  "keywords": [
    "ember-addon",
    "flatpickr",
    "datepicker",
    "datetime picker"
  ],
  "homepage": "https://shipshapecode.github.io/ember-flatpickr",
  "repository": "https://github.com/shipshapecode/ember-flatpickr",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.1.0",
    "flatpickr": "^4.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.47.0",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.2",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.4",
    "@types/ember__controller": "^3.16.6",
    "@types/ember__debug": "^3.16.5",
    "@types/ember__engine": "^3.16.2",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.3",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.9",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/ember__utils": "^3.16.2",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "ember-auto-import": "^2.2.0",
    "ember-cli": "~3.28.0",
    "ember-cli-addon-docs": "^4.0.3",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-data": "^3.25.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^6.2.2",
    "ember-power-select": "^4.0.3",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.2.3",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.11.6",
    "release-it-lerna-changelog": "^3.1.0",
    "typescript": "^4.1.5",
    "webpack": "^5.52.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://shipshapecode.github.io/ember-flatpickr"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "12.22.1",
    "yarn": "1.22.10"
  }
}
